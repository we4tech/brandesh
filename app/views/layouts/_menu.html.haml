.container{:role => "main"}
  %h1.visuallyhidden Entry Registration
  - if current_user
    = link_to 'Logout', destroy_user_session_path, :class => 'logout pull-right', :method => :delete
  - else
    = link_to 'Login', new_user_session_path, :class => 'logout pull-right'
  .row.margin_top_4
    %h2.visuallyhidden Registration Form
    / Aside
    %aside.span2{:role => "contemporary"}
      %h1.visuallyhidden User Sidebar
      %nav.sidenav
        %h1.visuallyhidden User Control
        %ul
          %li
            %i.icon-question-sign
            %a{"data-toggle" => "modal", :href => "#entryHelp"} Entry Help
          %li
            %i.icon-info-sign
            %a{"data-toggle" => "modal", :href => "#contestGuide"} Contest Guide
          %li
            %i.icon-calendar
            %a{:href => "#"} Schedule
          %li
            %i.icon-plus
            = link_to 'New Project', new_user_project_path(current_user)

    %p
      = notice
    %p
      = alert
    = yield